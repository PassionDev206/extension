{
    "manifest_version": 3,
    "name": "HTML Injector",
    "version": "1.0",
    "description": "Injects an HTML element into a website",
    "permissions": ["activeTab", "scripting"],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
        }
    },
    "content_scripts": [
        {
        "matches": ["https://bc.fun/game/crash"],
        "js": ["content.js"]
        }
    ]
}
